<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_margin="16dp"
    android:background="@color/cardview_dark_background"
    tools:context="zamboanga.antao.pockethajj.AnnualReport.Year2016Fragment">

    <!-- TODO: Update blank fragment layout -->



    <com.github.mikephil.charting.charts.PieChart
        android:id="@+id/chart2016"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.github.mikephil.charting.charts.PieChart>

    <TextView
        android:id="@+id/tvPilgrim2016"
        android:text="number of Pilgrim"
        android:textColor="@color/colorStroke"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tvHajjGuide2016"
        android:text="number of HajjGuide"
        android:textColor="@color/colorStroke"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
