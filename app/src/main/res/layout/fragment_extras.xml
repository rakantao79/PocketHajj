<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="zamboanga.antao.pockethajj.Fragments.ExtrasFragment">


    <ImageView
        android:id="@+id/imgVirtualTour"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="89dp"
        app:srcCompat="@drawable/logo_virtualtour"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imgQibla"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_below="@+id/imgVirtualTour"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="79dp"
        app:srcCompat="@drawable/logo_qibla" />
</RelativeLayout>
